{
  "name": "@sio/schema",
  "version": "2.0.0",
  "description": "Schema for values",
  "types": "dist/types",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
      ".": {
          "import": "./dist/mjs/index.js",
          "require": "./dist/cjs/index.js"
      }
  },
  "scripts": {
    "test-ts": "find ./src -name '*.test.ts' -type f -exec npx ts-node -O '{\"module\": \"commonjs\"}' '{}' \\;",
    "test": "tsc --noEmit && rm -rf test && tsc -p tsconfig.test.json && sh ./fixup.test.sh && node --test \"test/**/*.test.js\" && rm -rf test",
    "lint": "eslint --fix src/. && prettier --write src/.",
    "build": "rm -rf dist/* && tsc -p tsconfig.mjs.json && tsc -p tsconfig.cjs.json && sh ./fixup.sh"
  },
  "author": "Sergey Timonovskiy",
  "license": "ISC",
  "keywords": [
    "Validator",
    "Schema",
    "TypeScript"
  ],
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist"
  ]
}
